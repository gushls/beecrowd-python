valor = float(input())

notas = [100, 50, 20, 10, 5, 2]
moedas = [1, 0.50, 0.25, 0.10, 0.05, 0.01]

print("NOTAS:")
for nota in notas:
    notas = int(valor / nota)
    print('{} nota(s) de R$ {:.2f}'.format(notas, nota))
    valor -= notas * nota

print("MOEDAS:")
for moeda in moedas:
    valor = round(valor, 2)
    moedas = int(valor / moeda)
    print('{} moeda(s) de R$ {:.2f}'.format(moedas, moeda))
    valor -= moedas * moeda
